{
  "name": "cothinking-dev/deployer-laravel-stack",
  "description": "Deployer recipe for Laravel applications with PostgreSQL, Redis, and Caddy on Ubuntu",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "deployer",
    "laravel",
    "postgresql",
    "redis",
    "caddy",
    "deployment"
  ],
  "authors": [
    {
      "name": "Cothinking Dev",
      "email": "dev@cothinking.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "deployer/deployer": "^8.0@alpha"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "files": [
      "src/constants.php",
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Deployer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "deployer": {
      "recipes": [
        "src/recipe.php"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
